{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "commonjs": {
      "presets": ["@babel/preset-env"],
      "plugins": ["babel-plugin-add-module-exports"]
    },
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": ["babel-plugin-add-module-exports"]
    }
  }
}
